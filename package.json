  },
  "scripts": {
    "start": "node scripts/start.js --componentScssConfig scripts/ComponentScssConfig.js",
    "lib": "node scripts/lib.js --componentScssConfig scripts/ComponentScssConfig.js",
    "build": "GENERATE_SOURCEMAP=false node scripts/build.js  --componentScssConfig scripts/ComponentScssConfig.js ",
    "test": "CI=true node scripts/test.js --env=jsdom",
    "rollupbuild": "node scripts/rollup.build.js",
    "rollupbuildserve": "serve -s build -p 8082",
    "fullstart": "npm run build && npm run rollupbuild && npm run rollupbuildserve",
    "pre-commit": "npm run build",
    "pre-push": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "husky": "^3.0.5",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "typescript": "^3.0.3"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/setUp.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  }
}
